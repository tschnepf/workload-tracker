{
    "name":  "workload-tracker-frontend",
    "private":  true,
    "version":  "0.0.0",
    "type":  "module",
    "scripts":  {
                    "dev":  "vite",
                    "build":  "tsc --noEmit \u0026\u0026 vite build",
                    "build:analyze":  "vite build \u0026\u0026 echo \u0027Bundle analysis generated at dist/stats.html\u0027",
                    "lint":  "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
                    "lint:soft":  "eslint . --ext ts,tsx --report-unused-disable-directives",
                    "preview":  "vite preview",
                    "test":  "vitest",
                    "test:ui":  "vitest --ui",
                    "test:run":  "vitest run",
                    "e2e":  "playwright test",
                    "e2e:ui":  "playwright test --ui",
                    "e2e:codegen":  "playwright codegen",
                    "openapi:types":  "openapi-typescript ../backend/openapi.json -o src/api/schema.ts",
                    "openapi:regen":  "npm run openapi:types"
                },
    "overrides":  {
                      "esbuild":  "0.25.9"
                  },
