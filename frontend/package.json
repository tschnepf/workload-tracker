                         "openapi-fetch":  "0.9.2",
                         "react":  "19.1.1",
                         "react-dom":  "19.1.1",
                         "react-router":  "7.9.1",
                         "serve":  "14.2.5",
                         "web-vitals":  "5.1.0"
                     },
